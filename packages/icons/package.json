{
	"name": "@tokens-studio/icons",
	"version": "0.1.3",
	"description": "Tokens Studio: Internal icon library",
	"author": "six7@hyma.io",
	"type": "module",
	"exports": {
		"./*": {
			"import": "./dist/icons/*",
			"types": "./dist/icons/*"
		},
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"build": "tsc && pnpm run format:prettier",
		"clean": "rm -rf src/icons && rm -rf dist",
		"dev": "tsc --watch",
		"regenerate": "pnpm run clean && pnpm run svgr",
		"svgr": "svgr --config-file svgr.config.cjs assets",
		"format": "pnpm run format:eslint && pnpm run format:prettier",
		"format:eslint": "eslint . --fix",
		"format:prettier": "prettier . --write",
		"lint": "pnpm run lint:prettier && pnpm run lint:eslint && pnpm run lint:types",
		"lint:eslint": "eslint .",
		"lint:prettier": "prettier . --check",
		"lint:types": "tsc --noEmit",
		"release": "pnpm run regenerate && pnpm run build && changeset publish"
	},
	"peerDependencies": {
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.9",
		"@svgr/cli": "^8.0.1",
		"@tokens-studio/configs": "^0.2.3",
		"eslint": "^9.11.1",
		"prettier": "^3.3.3",
		"typescript": "^5.6.2"
	}
}
